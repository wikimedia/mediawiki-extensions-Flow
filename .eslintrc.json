{
	"extends": "wikimedia",
	"env": {
		"browser": true
	},
	"globals": {
		"mw": true,
		"$": false,
		"OO": false,
		"Handlebars": false,
		"moment": false
	},
	"rules": {
		"dot-notation": [ "error", { "allowKeywords": true } ],
		"no-underscore-dangle": 0,
		"camelcase": 0,
		"no-use-before-define": 0,
		"max-len": 0
	}
}
