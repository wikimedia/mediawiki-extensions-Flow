{
	"root": true,
	"extends": [
		"wikimedia/client",
		"wikimedia/jquery",
		"wikimedia/mediawiki"
	],
	"globals": {
		"Handlebars": "readonly",
		"moment": "readonly"
	},
	"rules": {
		"no-underscore-dangle": "off",
		"camelcase": "off",
		"max-len": "off",
		"no-shadow": "warn",
		"no-jquery/no-extend": "warn",
		"es-x/no-object-assign": "warn",
		"prefer-const": "warn",
		"no-jquery/no-done-fail": "warn"
	}
}
