[access]
	inheritFrom = mediawiki/extensions
[project]
[access "refs/*"]
	owner = group extension-Flow
[receive]
	requireChangeId = true
[submit]
	mergeContent = true
[access "refs/meta/config"]
	push = group extension-Flow
[dashboard]
	default = refs/meta/dashboards/custom:custom
[access "refs/heads/master"]
	exclusiveGroupPermissions = label-Verified submit
	label-Verified = -1..+2 group JenkinsBot
	label-Verified = -1..+2 group l10n-bot
	label-Verified = -1..+1 group Registered Users
	submit = deny group Registered Users
	submit = group JenkinsBot
	submit = group l10n-bot
[allowSuperproject "mediawiki/extensions"]
	matching = refs/heads/REL1_27:refs/heads/REL1_27
[allowSuperproject "mediawiki/core"]
	matching = refs/heads/wmf/1.29.0-wmf.1:refs/heads/wmf/1.29.0-wmf.1
